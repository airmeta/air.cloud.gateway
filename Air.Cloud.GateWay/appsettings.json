{
  //Consul����
  "ConsulServiceOptions": {
    "ConsulAddress": "http://192.168.100.154:8500/",
    "ServiceName": "FCJ.GateWay"
  },
  "AllowCors": "http://192.168.110.130:8080,http://192.168.110.130:8080/,http://localhost:8080",
  //授权服务配置
  "AuthorizationSettings": {
    //是否启用授权服务
    "EnableAuthorizationService": true,
    //用户访问Token标识
    "AuthorizationHeader": "Authorization",
    //用户刷新Token标识
    "XAuthorizationHeader": "X-Authorization",
    //授权服务地址
    "AuthorizationService": {
      //授权服务IP
      "ServiceIP": "127.0.0.1",
      //授权服务端口
      "ServicePort": 30001
    },
    //白名单请求头 配置此配置项的请求头不会被拦截
    "WhiteHeader": "Launcher"
  },
  "AppSettings": {
    "Certs": {
      "Name": "xx",
      "Password": "xx"
    }
  },
  "Environment": "Development"
}